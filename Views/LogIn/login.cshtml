    <!-- why section -->
    <section class="why_section layout_padding">
        <div class="container">

            <div class="row">
                <div class="col-lg-8 offset-lg-2">
                    <div class="full">
                        <form action="/LogIn/login" method="post">
                            <input type="email" placeholder="Enter your email" name="email" />
                            <input type="password" placeholder="Enter your passowrd" name="password" />

                            <button class="btn px-2 btn-success text-white " type="submit">
                                LogIn
                            </button>



                        </form>
                        <p style="inline-block">
                            Don't have an account?
                            <a href="/SignUp/signup">
                                <button class="btn  text-success ">Signup</button>
                            </a>
                        </p>
                        <p style="inline-block">
                            Login as Admin?
                            <a href="/Admin/admin">
                                <button class="btn  text-success ">Login</button>
                            </a>
                        </p>
                    </div>
                    
                   
                </div>
            </div>
        </div>
    </section>
    <!-- end why section -->
